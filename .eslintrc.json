{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true,
    "commonjs": true,
    "cypress/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:cypress/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "js": true,
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "plugins": ["react", "cypress"],
  "rules": {
    "react/prop-types": "off",
    "consistent-return": 2
  }
}
